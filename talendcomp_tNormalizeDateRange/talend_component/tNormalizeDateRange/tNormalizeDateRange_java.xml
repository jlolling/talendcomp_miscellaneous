<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER AUTHOR="Jan Lolling" COMPATIBILITY="ALL"
		SCHEMA_AUTO_PROPAGATE="false" DATA_AUTO_PROPAGATE="false"
		IS_MULTIPLYING_OUTPUTS="true" PLATEFORM="ALL" RELEASE_DATE="20160211A"
		SERIAL="" STARTABLE="false" STATUS="PROD" VERSION="1.1">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Processing/Fields</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1"
			MIN_OUTPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="DATE_RANGE_START_COLUMN" FIELD="PREV_COLUMN_LIST"
			REQUIRED="true" NUM_ROW="20">
		</PARAMETER>

		<PARAMETER NAME="EXCLUDE_START_DATE" FIELD="CHECK"
			REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATE_RANGE_END_COLUMN" FIELD="PREV_COLUMN_LIST"
			REQUIRED="true" NUM_ROW="30">
		</PARAMETER>

		<PARAMETER NAME="CONFIGTEXT" FIELD="LABEL" COLOR="0;0;0"
			NUM_ROW="40">
			<DEFAULT>Please add to the output schema a column which will get the current date.</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CURRENT_DATE_COLUMN" FIELD="COLUMN_LIST"
			REQUIRED="true" NUM_ROW="50">
		</PARAMETER>

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="TRUNCATE_CURRENT_DATE_TIME" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="10">
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt-talendcomp-normalizedaterange" MODULE="cimt-talendcomp-normalizedaterange-2.0.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE_IN" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE_OUT" TYPE="id_Integer" />
		<RETURN AVAILABILITY="FLOW" NAME="INDEX_WITHIN_RANGE" TYPE="id_Integer" />
	</RETURNS>
</COMPONENT>
